NAME = ft_display_file

all: $(NAME)

$(NAME):
	gcc -Wall -Wextra -Werror -c main.c
	gcc -Wall -Wextra -Werror -o $(NAME) main.o

clean:
	rm -f main.o

fclean: clean
	rm -f $(NAME)


